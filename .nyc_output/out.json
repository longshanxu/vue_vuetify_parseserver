        "<!--\r\n * @Author: Json.Xu\r\n * @Date: 2019-11-26 10:13:57\r\n * @LastEditTime: 2019-11-27 18:40:46\r\n * @LastEditors: Json.Xu\r\n * @Description: \r\n * @FilePath: \\moch-vue-mobile\\src\\views\\SignUp.vue\r\n -->\r\n<template>\r\n  <v-container class=\"pa-0\">\r\n    <v-card class=\"overflow-hidden\" flat>\r\n      <v-app-bar fixed dark color=\"primary\" class=\"pa-0\" dense scroll-target=\"#scrolling-sheet\">\r\n        <v-row align=\"center\" justify=\"center\" dense>\r\n          <v-col cols=\"2\">\r\n            <router-link to=\"/login\">\r\n              <v-btn icon>\r\n                <v-icon>mdi-arrow-left</v-icon>\r\n              </v-btn>\r\n            </router-link>\r\n          </v-col>\r\n          <v-col cols=\"8\" style=\"text-align:center\">\r\n            <v-toolbar-title class=\"white--text\">注册</v-toolbar-title>\r\n          </v-col>\r\n          <v-col cols=\"2\">\r\n            <v-spacer></v-spacer>\r\n          </v-col>\r\n        </v-row>\r\n      </v-app-bar>\r\n      <v-sheet id=\"scrolling-sheet\" class=\"my-12\">\r\n        <v-container>\r\n          <v-card class=\"mx-auto\" flat v-show=\"sicode\">\r\n            <v-card-text>\r\n              <v-form>\r\n                <v-text-field\r\n                  outlined\r\n                  single-line\r\n                  placeholder=\"请输入手机号码\"\r\n                  name=\"tel\"\r\n                  prepend-inner-icon=\"mdi-account\"\r\n                  type=\"text\"\r\n                ></v-text-field>\r\n                <v-text-field\r\n                  outlined\r\n                  single-line\r\n                  placeholder=\"请输入验证码\"\r\n                  prepend-inner-icon=\"mdi-lock\"\r\n                  name=\"code\"\r\n                  type=\"password\"\r\n                >\r\n                  <template v-slot:append>\r\n                    <v-btn\r\n                      text\r\n                      color=\"primary\"\r\n                      height=\"24\"\r\n                      style=\"border-left:1px solid #ccc ;\"\r\n                    >获取验证码</v-btn>\r\n                  </template>\r\n                </v-text-field>\r\n              </v-form>\r\n            </v-card-text>\r\n            <v-card-actions class=\"pt-0\">\r\n              <v-card-text>\r\n                <v-btn block color=\"primary\" @click=\"signcode\">下一步</v-btn>\r\n              </v-card-text>\r\n            </v-card-actions>\r\n          </v-card>\r\n          <v-card class=\"mx-auto\" flat v-show=\"sipwd\">\r\n            <v-card-text>\r\n              <v-form>\r\n                <v-text-field\r\n                  outlined\r\n                  single-line\r\n                  placeholder=\"请输入密码（8-20位数字或字母）\"\r\n                  name=\"pwd\"\r\n                  prepend-inner-icon=\"mdi-lock\"\r\n                  type=\"password\"\r\n                ></v-text-field>\r\n                <v-text-field\r\n                  outlined\r\n                  single-line\r\n                  placeholder=\"请输入密码（8-20位数字或字母）\"\r\n                  prepend-inner-icon=\"mdi-lock\"\r\n                  name=\"pwdagain\"\r\n                  type=\"password\"\r\n                ></v-text-field>\r\n              </v-form>\r\n            </v-card-text>\r\n            <v-card-actions class=\"pt-0\">\r\n              <v-card-text>\r\n                <v-btn block color=\"primary\" @click=\"signpwd\">下一步</v-btn>\r\n              </v-card-text>\r\n            </v-card-actions>\r\n          </v-card>\r\n          <v-card class=\"mx-auto\" flat v-show=\"siname\">\r\n            <v-card-text>\r\n              <v-form>\r\n                <v-text-field\r\n                  outlined\r\n                  single-line\r\n                  placeholder=\"请输入真实姓名便于创建/加入公司\"\r\n                  name=\"company\"\r\n                  prepend-inner-icon=\"mdi-account\"\r\n                  type=\"text\"\r\n                ></v-text-field>\r\n              </v-form>\r\n            </v-card-text>\r\n            <v-card-actions class=\"pt-0\">\r\n              <v-card-text>\r\n                <v-btn block color=\"primary\" to=\"/elevator\">下一步</v-btn>\r\n              </v-card-text>\r\n            </v-card-actions>\r\n          </v-card>\r\n\r\n          <v-footer fixed>\r\n            <v-btn text color=\"primary\" class=\"mx-auto\">\r\n              <span style=\"color:grey\">注册代表您已阅读并同意</span>《佳格维保协议》\r\n            </v-btn>\r\n          </v-footer>\r\n        </v-container>\r\n      </v-sheet>\r\n    </v-card>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      sicode: true,\r\n      sipwd: false,\r\n      siname: false\r\n    };\r\n  },\r\n  methods: {\r\n    signcode() {\r\n      this.sicode = false;\r\n      this.sipwd = true;\r\n      this.siname = false;\r\n    },\r\n    signpwd() {\r\n      this.sicode = false;\r\n      this.sipwd = false;\r\n      this.siname = true;\r\n    }\r\n    // signname() {\r\n\r\n    // }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n</style>"
